<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/faculty_navigation"
    app:startDestination="@+id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.tabbedactivities.faculty.FacultyFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home_faculty" />
    <fragment
        android:id="@+id/nav_extension"
        android:name="com.example.tabbedactivities.faculty.FacultyExtension"
        android:label="Extension Forms"
        tools:layout="@layout/fragment_extension_forms" />
    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.tabbedactivities.ui.logout.Logout"
        android:label="Logout"
        tools:layout="@layout/fragment_logout"
        />
    <fragment
        android:id="@+id/nav_feedback"
        android:name="com.example.tabbedactivities.ui.feedback.FeedbackFragment"
        android:label="FeedBack"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/nav_report"
        android:name="com.example.tabbedactivities.ui.report.ReportFragment"
        android:label="Report"
        tools:layout="@layout/fragment_report" />
